<?xml version="1.0" encoding="utf-8"?>
<document xmlns="http://maven.apache.org/DOCUMENT/1.0.1"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/DOCUMENT/1.0.1 http://maven.apache.org/xsd/document-1.0.1.xsd"
outputName="maven-pdf-plugin">
  <!-- see: https://maven.apache.org/doxia/doxia/doxia-core/document.html -->
  <meta>
    <title>Project documentatie voor ${project.name}</title>
    <authors>
      <author>
        <name>${project.developers[0].name}</name>
      </author>
      <author>
        <name>${project.developers[1].name}</name>
      </author>
    </authors>
    <keywords>a11y</keywords>
    <description>${project.description}</description>
    <!-- date>${date}</date -->
  </meta>
  <toc name="Inhoud">
    <item name="Home" href="/index.html" />
    <item name="Release notes" href="/releasenotes.html" />
    <item name="Functioneel ontwerp" href="/functioneelontwerp.html" />
    <item name="Technisch ontwerp" href="/technischontwerp.html" />
    <item name="Ontwikkel handleiding" href="/ontwikkelhandleiding.html" />
    <item name="Installatie handleiding" href="/installatiehandleiding.html" />
  </toc>
  <cover>
    <coverTitle>${project.name}</coverTitle>
    <coverSubTitle>Project documentatie</coverSubTitle>
    <coverVersion>${project.version}</coverVersion>
    <coverType>User Guide</coverType>
    <projectName>${project.name}</projectName>
    <projectLogo />
    <companyName>${project.organization.name}</companyName>
    <companyLogo>images/logo.png</companyLogo>
    <coverdate>${date}</coverdate>
  </cover>
</document>
